<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="partkeepr.tip_of_the_day_service" class="PartKeepr\TipOfTheDayBundle\Services\TipOfTheDayService">
            <argument type="service" id="service_container"/>
            <argument type="service" id="doctrine.orm.entity_manager"/>
            <argument type="service" id="partkeepr.cronlogger_service"/>
            <argument type="service" id="partkeepr.remote_file_loader.factory"/>
        </service>
    </services>
</container>
